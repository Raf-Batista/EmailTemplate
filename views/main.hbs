        {{/each}}
    </div>

    <hr>

    <div class="center-btn">
      <button>LEARN MORE</button>
    </div>

    <div class="social-media-icons">
        <i class="fab fa-facebook"></i>
        <i class="fab fa-twitter"></i>
        <i class="fab fa-instagram-square"></i>
    </div>

     <footer>
